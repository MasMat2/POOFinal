package practica4;

public class Numbers {
	
	public int compare(int a, int b) {
		if(a>=b) {
			return a;
		}else {
			return b;
		}
	}
	public int compare(int a, int b, int c) {
		return compare(compare(a,b),c);
		}
	public float iva(float precio, int cantidad) {
		return (float) precio*cantidad*0.14;
	}
}
