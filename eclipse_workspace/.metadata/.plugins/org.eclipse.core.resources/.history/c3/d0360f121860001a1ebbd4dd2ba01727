package practica4;

public class Numbers {
	
	public int compare(int a, int b) {
		if(a>=b) {
			return a;
		}else {
			return b;
		}
	}
	public int compare(int a, int b, int c) {
		return compare(compare(a,b),c);
		}
	public float iva(float precio, int cantidad) {
		float tax = (float) (precio*cantidad*0.14);
		return tax;
	}
	public float iva(int cantidad, float precio) {
		return iva(precio, cantidad);
	}
}
